{
  "name": "extra-ecs",
  "version": "0.3.3",
  "description": "",
  "keywords": [],
  "files": [
    "lib",
    "dist"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "repository": "git@github.com:BlackGlory/extra-ecs.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "deduplicate": "yarn-deduplicate",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "jest --runInBand --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:coverage": "jest --coverage --config jest.config.js",
    "prepublishOnly": "run-s clean build bundle",
    "clean": "run-p clean:*",
    "clean:build": "rimraf lib",
    "clean:bundle": "rimraf dist",
    "build": "run-p build:*",
    "build:compile": "tsc --project tsconfig.build.json --module commonjs",
    "build:patch": "tscpaths -p tsconfig.build.json -s ./src -o ./lib",
    "bundle": "rollup --config rollup.config.js",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint build test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@blackglory/jest-matchers": "^0.5.0",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.4.0",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "husky": "^4.3.8",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "return-style": "^1.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.78.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "standard-version": "^9.5.0",
    "structure-of-arrays": "^0.5.12",
    "ts-jest": "^29.0.3",
    "tscpaths": "^0.0.9",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4",
    "yarn-deduplicate": "^6.0.0"
  },
  "dependencies": {
    "@blackglory/prelude": "^0.1.8",
    "@blackglory/structures": "^0.11.4",
    "hotypes": "^0.5.0",
    "iterable-operator": "^2.3.0",
    "justypes": "^3.0.0"
  },
  "peerDependencies": {
    "structure-of-arrays": "^0.5.11"
  }
